{
    "telemetry.telemetryLevel": "off",
    "editor.fontFamily": "JetBrains Mono,Noto Color Emoji",
    "editor.rulers": [80],
    "files.trimTrailingWhitespace": true,
    "workbench.colorTheme": "One Dark Pro",
    "workbench.iconTheme": "vscode-icons",
    "oneDarkPro.bold": true,
    "oneDarkPro.italic": true,
    "oneDarkPro.vivid": true,
    "oneDarkPro.markdownStyle": true,
    "markdown.preview.typographer": true,
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "gitlens.currentLine.format": "${agoOrDate}${ â€¢ message|50?}",
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.views.commits.avatars": false,
    "cSpell.languageSettings": [
        {
            "languageId": "markdown",
            "ignoreRegExpList": [ "/^\\s*```[\\s\\S]*?^\\s*```/gm", "/`.*`/gm" ]
        }
    ]
}
